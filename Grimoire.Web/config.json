{
    "AllowedHosts": "*",
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft.AspNetCore": "Warning",
            "System.Net.Http": "Information"
        }
    },
    "Kestrel": {
        "Endpoints": {
            "Http": {
                "Url": "http://0.0.0.0:9000"
            },
            "Https": {
                "Url": "http://0.0.0.0:9001"
            }
        }
    },
    "Theme": "dark",
    "RefreshEvery": "00:30:00",
    "Save": {
        "To": "Grimoire_Data",
        "SourceIcon": true,
        "MangaCover": true,
        "MangaChapter": true
    },
    "Cache": {
        "Manga": true
    },
    "Mongo": "mongodb://grimoire:grimoire@localhost:27017"
}