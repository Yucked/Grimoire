{
    "AllowedHosts": "*",
    "Logging": {
        "LogLevel": {
            "Default": "Trace",
            "Microsoft.AspNetCore": "Warning"
        }
    },
    "Kestrel": {
        "Endpoints": {
            "Http": {
                "Url": "http://0.0.0.0:5000"
            },
            "Https": {
                "Url": "http://0.0.0.0:5001"
            }
        }
    },
    "Theme": "light",
    "RefreshEvery": "00:30:00",
    "Save": {
        "To": "Grimoire_Data",
        "SourceIcon": true,
        "MangaCover": true,
        "MangaChapter": true
    },
    "Cache": {
        "Manga": true
    }
}